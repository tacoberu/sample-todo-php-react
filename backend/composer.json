{
    "require": {
		"php": ">=8.2",
        "tracy/tracy": "^2.10",
        "nette/utils": "^4.0"
    },
	"autoload": {
		"classmap": [
			"app/"
		]
	},
	"minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "phpstan/phpstan": "^1.12"
    },
	"scripts": {
		"phpunit": [
			"vendor/bin/phpunit"
		],
		"phpstan": [
			"vendor/bin/phpstan analyse --memory-limit=512M"
		]
	}
}
